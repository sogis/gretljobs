MODEL NABODAT_ErgebnisseBodenbelastung_Punktdaten_LV03_V1_1 (de)
AT "https://models.geo.admin.ch/BAFU"
VERSION "2020-01-29" =

  IMPORTS Units,CHAdminCodes_V1,GeometryCHLV03_V1,ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1;
  
  /* Einheit Volumen Vorrat Wald und Anzahl Jahre ergänzt -> im MGDM nicht nötig
   */
  UNIT
    m3pha = (Units.m3 / Units.ha);
    Jahre;
  
!! ========================================================================
  TOPIC Punktdaten =
    DEPENDS ON ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProjektstandort,ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten,ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenAnalysedaten;

    /** geologisches Ausgangsmaterial der Bodenbildung
     */
    STRUCTURE Ausgangsmaterial =
      /** Profilblatt FAL, Feld 62
       */
      Ausgangsmaterial : MANDATORY ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProjektstandort.Ausgangsmaterial_Ref;
      /** Profilblatt FAL, Feld 63
       */
      Eiszeit : ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProjektstandort.Eiszeit_Ref;
    END Ausgangsmaterial;

    /** Zusatzattribute aus BICH Migraprofil
     */
    CLASS BICHQualitaet =
      Lagegenauigkeit : 0 .. 1000 [INTERLIS.m];		!! klle: in NABODAT kein Bereich definiert
      DatenkontrolleDatum : FORMAT INTERLIS.XMLDate "1900-1-1" .. "2050-1-1";
      DatenkontrollePruefstelle : TEXT*60;
      DatenkontrollePedologe : TEXT*60;
      Bemerkung : MTEXT*500;
      StandortAktualitaetDatum : FORMAT INTERLIS.XMLDate "1900-1-1" .. "2050-1-1";
      Bearbeitungstand : ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.Bearbeitungsstand_Ref;
      /** Profilblatt FAL, Feld 1
       * Klassifikationssystem vor Migration auf DS 6 
       */
      Klassifikationssystem : ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.Klassifikationssystem_Ref;
      StandortAktualitaetBeurteilung : ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.StandortAktualitaetBeurteilung_Ref;
    END BICHQualitaet;

		/* Bei der Bodenfarbe gibt es einige Pflichtfelder in NABODAT. Fürs MGDM habe ich das Interlismodell nicht angepasst, da diese Attribute ja ausgefüllt sein müssten und somit immer ausgegeben werden.
		 */
    CLASS Bodenfarbe =
      Farbbezug : ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.Farbbezug_Ref;
      FarbtonText : MANDATORY ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.FarbtonText_Ref;  !! klle: Pflichtfeld NABODAT
      FarbtonZahl : ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.FarbtonZahl_Ref;
      Helligkeit : MANDATORY ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.Helligkeit_Ref;  !! klle: Pflichtfeld NABODAT
      Intensitaet : MANDATORY ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.Intensitaet_Ref;  !! klle: Pflichtfeld NABODAT
    END Bodenfarbe;

    STRUCTURE BodenskelettFeldBereich =
      VolumenVon : 0 .. 100 [Units.Percent];
      VolumenBis : 0 .. 100 [Units.Percent];
      steinhaltig : BOOLEAN;
      Skelettgehalt : ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.Skelettgehalt_Ref;
    END BodenskelettFeldBereich;

    /* Macht es Sinn diese Attribute zu den Dokumenten auszugeben, auch wenn die Dokumente nicht mit dem MGDM ausgegeben werden?
     * Wie besteht die Verknüpfung zwischen Projekt, Standort oder Profil, in der DB gibt es noch die Zwischentabelle, welche hier fehlt!
     */
    CLASS Dokument =
      /** Titel des Dokuments
       */
      Titel : MANDATORY MTEXT*255;
      Bemerkung : MTEXT*255; 	!! klle: Länge Textfeld angepasst
      /** Dateiname des Dokuments
       */
      originalDokumentName : MANDATORY MTEXT*255;
      DokumentTyp : MANDATORY ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProjektstandort.DokumentTyp_Ref;
      /* klle: Wie muss ich die Zugangsstufe einbinden? Als Codeliste oder nur als Wert? -> evtl. später noch anpassen
      */
      Zugangsstufe : MANDATORY ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProjektstandort.Zugangsstufe_Ref;	!! klle: zusätzliches Attribut NABODAT
    END Dokument;

    /** Dient der Erfassung
     * - der Untersuchungskampagnen bei Dauerbeobachtungsflaechen
     * - der Erhebungskampagnen (Lose) bei Kartieretappen (im PDM fuer Profilaufnahmen)
     * 
     * Alte Bezeichnung fuer diese Klasse = Probenahme.
     * 
     * Der Schluessel setzt sich aus ErhebungsNr und ProfilID zusammen.
     */
    CLASS Erhebung =
      /** Nummer der Erhebung;
       * alter Attributname = Probenahmenummer
       */
      ErhebungsNr : MANDATORY 0 .. 1000;
      /** Datum der Erhebung, bzw. Probenahme;
       * alter Attributnahme = Probenahmedatum
       */
      ErhebungsDatum : MANDATORY FORMAT INTERLIS.XMLDate "1900-1-1" .. "2050-1-1";
      /** Name oder Kuerzel des Probenehmers, bzw. des Pedologen
       */
      Probenehmer : TEXT*60;
      Bemerkung : MTEXT*500;
      /** Notiz zur Bodenbearbeitung am Probenahmestandort, bzw. der Beobachtungsflaeche
       */
      Bodenbearbeitung : MTEXT*200;
      /** Notiz zur Bodenfeuchtigkeit bei der Probenahme am Probenahmestandort, bzw. der Beobachtungsflaeche
       */
      Bodenfeuchtigkeit : MTEXT*200;
      /** Nummer des "Miniprofils".
       * Die ProfilId ermaeglicht die Erfassung von mehreren "Miniprofilen" innerhalb einer Erhebung (z.B. Humax, Splittube in BDF). Dabei werden i.d.R. Horizontbezogene Mischproben erstellt.
       */
      ProfilID : MANDATORY TEXT*60;
      /** FAL-Profilblatt, Feld 3;
       * Die Codeliste wurde erweitert um die Eintraege "Splittube" und "ohne Horizontbezug"
       */
      Erhebungsart : MANDATORY ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProjektstandort.Erhebungsart_Ref;
      /* zusätzliche Attribute für Export NABODAT
      */
      Lufttemperatur : -100 .. 100 [Units.oC];
      Witterung : MTEXT*200;
      Zugangsstufe : MANDATORY ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProjektstandort.Zugangsstufe_Ref;	!! klle: zusätzliches Attribut NABODAT
    END Erhebung;

    /** Die Klasse ErhebungsLos hat ihren Ursprung im KartierLos (Anlass, Name, beteiligte Personen/Unternehmen usw.) der Bodenkartierung und enthaelt nur jene Attribute die auch in NABODAT an dieser Stelle uebernommen wurden. Daten die mit der Partner-, bzw. Adressdatenbank verknuepft wurden sind im MGDM nicht enthalten. Um auch entsprechende Daten von Bodendauerbeobachtungsflaechen erfassen zu kaennen, ist diese Klasse an die Erhebung gekoppelt. Grund: Bei BDF kann der Fall eintreten, dass der gleich Standort in unterschiedlichen Erhebungsjahren, durch unterschiedliche Unternehmen beprobt wird.
     * klle: Seit 1.4 nur noch für Kartierungsdaten gebraucht -> für NABODAT-Export nicht nötig, für MGDM sind Name und verantwortliche Person verlangt
     */
    CLASS ErhebungsLos =
      /** Name des Loses, z.B. "BOKA LU 2011 Los 3"
       */
      ErhebungsLosName : MANDATORY TEXT*60;
      /** verantwortliche Person auftragnehmerseitig; Projektleiter Kartierlos
       */
      verantwortlichePerson : TEXT*60;
    END ErhebungsLos;

    CLASS Gefuege =
      Form : MANDATORY ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.Form_Ref;  !! klle: Pflichtfeld ergänzt für NABODAT
      Groesse : ErgebnisseBodenbelastung_Punktdaten_Codelisten_V1_1.CodelistenProfildaten.Groesse_Ref;
    END Gefuege;

